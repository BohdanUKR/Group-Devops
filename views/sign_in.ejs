        <label>Password:</label>
        <input type="text" placeholder="Enter password" id="upass" name="Password" required>
        <br>
        <input type="submit" value="Login">
      </form>
    </div>
    <div>
      <button><a href="/signup">Sign up</a></button>
    </div>
  </body>
</html>

