<div class="container-fluid">
  <table class="table table-hover rounded-circle ">
           <tr class="d-flex">
               <th class="text-center col-3">First Name</th>
               <th class="text-center col-3">Last Name</th>
               <th class="text-center col-2">Skills</th>
               <th class="text-center col-2">Complexity</th>
               <th class="text-center col-2">Department</th>
           </tr>
            <% for (let count = 0; count < Professors.length; count++) { %>
              <tr class="d-flex">
                 <td class="text-center col-3"><%= Professors[count].FirstName %></td>
                 <td class="text-center col-3"><%= Professors[count].LastName %></td>
                 <td class="text-center col-2"><%= Professors[count].StarsSkills %></td>
                 <td class="text-center col-2"><%= Professors[count].StarsComplexity %></td>
                 <td class="text-center col-2"><%= Professors[count].Department %></td>
              </tr>
          <% } %>
 </table>
</div>





